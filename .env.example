# Server Configuration
PORT=6090

# Database
DB_URL=mongodb+srv://username:password@cluster.mongodb.net/database_name

# JWT Configuration
JWT_SECRETKEY=your_jwt_secret_key_here

# Environment
NODE_ENV=development
NODE_VERSION=21.7.3

# CORS Configuration - Comma-separated list of allowed origins
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:5501,http://localhost:5173,http://localhost:5500,http://127.0.0.1:5500,http://localhost:5501,null

# Cloudinary Configuration
CLOUDINARY_NAME=your_cloudinary_cloud_name
CLOUDINARY_API_KEY=your_cloudinary_api_key
CLOUDINARY_API_SECRET=your_cloudinary_api_secret

# Swagger Configuration
SWAGGER_SERVER=http://localhost:6090/v1
SECOND_SWAGGER_SERVER=https://your-production-url.com

# Email Configuration
emailAddress=your_email@example.com
emailpassword=your_app_password
password=your_password

# Twilio Configuration
TWILIO_ACCOUNT_SID=your_twilio_phone_number
TWILIO_AUTH_TOKEN=your_twilio_auth_token
username=your_username
