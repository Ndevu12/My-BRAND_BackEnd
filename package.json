    "description": "Welcome to the My-BRAND_BackEnd repository! This repository contains the backend codebase for the My-BRAND project.",
    "main": "./dist/app.js",
    "scripts": {
        "test": "echo `Test not specified` && exit 1",
        "dev": "nodemon --exec ts-node src/app.ts",
        "start": "node ./dist/app.js",
        "build": "npm install && npm audit fix --force && rimraf dist && tsc",
        "seed": "ts-node src/seeders/index.ts",
        "seed:categories": "ts-node src/seeders/index.ts categories",
        "seed:users": "ts-node src/seeders/index.ts users",
        "seed:profiles": "ts-node src/seeders/index.ts profiles",
        "seed:blogs": "ts-node src/seeders/index.ts blogs",
        "seed:categories:force": "ts-node src/seeders/index.ts categories --force",
        "seed:users:force": "ts-node src/seeders/index.ts users --force",
        "seed:profiles:force": "ts-node src/seeders/index.ts profiles --force",
        "seed:blogs:force": "ts-node src/seeders/index.ts blogs --force",
        "db:clear": "ts-node src/seeders/index.ts --clear-all",
        "db:reset": "ts-node src/seeders/index.ts --reset-db",
        "db:fresh": "ts-node src/seeders/index.ts all --clear-all"
    },
